CSS Reset
*********************/

html, body, div, span, applet, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address, big, cite, code,
del, dfn, em, img, ins, kbd, q, s, samp,
small, strike, strong, sub, sup, tt, var,
b, u, i, center,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, tbody, tfoot, thead, tr, th, td,
article, aside, canvas, details, embed, 
figure, figcaption, footer, header, hgroup, 
menu, nav, output, ruby, section, summary,
time, mark, audio, video {
	margin: 0;
	padding: 0;
	border: 0;
	font-size: 100%;
	font: inherit;
	vertical-align: baseline;
}
/* HTML5 display-role reset for older browsers */
article, aside, details, figcaption, figure, 
footer, header, hgroup, menu, nav, section {
	display: block;
}
body {
	line-height: 1;
}
ol, ul {
	list-style: none;
}


/*********
SASS VARIABLES
*********/
$primarycolor: #f4f1de; /* text color */
$secondarycolor: #0eadff; /* background color */
$accentcolor: #EC6E61; /* accent color */
$accentcolortwo: #fff9a4;
$paragraphfont: 'quicksand','myriad-pro-1','myriad-pro-2','Lucida Grande',Sans-Serif;
$headingfont: 'bebas', sans-serif;
$linkcolor: #f4f1de;
/**************
web fonts
**************/
@font-face {
    font-family: 'bebas';
    src: url('../fonts/BebasNeue-webfont.eot');
    src: url('../fonts/BebasNeue-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/BebasNeue-webfont.woff') format('woff'),
         url('../fonts/BebasNeue-webfont.ttf') format('truetype'),
         url('../fonts/BebasNeue-webfont.svg#bebas_neueregular') format('svg');
    font-weight: normal;
    font-style: normal;

}


@font-face {
    font-family: 'quicksand';
    src: url('../fonts/quicksand-light-webfont.eot');
    src: url('../fonts/quicksand-light-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/quicksand-light-webfont.woff') format('woff'),
         url('../fonts/quicksand-light-webfont.ttf') format('truetype'),
         url('../fonts/quicksand-light-webfont.svg#quicksandlight') format('svg');
    font-weight: normal;
    font-style: normal;

}
/*********************
START OF MY STYLING
*********************/
/*********************
Basic Styles
*********************/
body {
	background-color: $secondarycolor;
	line-height: 1.4em;
	font-size: 100%;
	font-family: $paragraphfont;
}

#wrapper {
	width: 75.949367088607594936708860759494%; /* 960px/1280px */
	margin: 0 auto;
}
.display-none {
	display: none;
}
h1 {
	font-size: 4em; /* 32px/16px */
	line-height: 50px;
	color: $primarycolor;

	margin-bottom: 25px;
	-webkit-column-span: all;
	-moz-column-span: all;
	-o-column-span: all;
	column-span: all;
}

h2 {
	font-size: 2.5em; /* 28px/16px */
	color: $primarycolor;
	text-transform: none;
	margin: 10px 0px;
	}

h3 {
	font-size: 2em; /* 20px/16px */
	color: $primarycolor;
	text-transform: none;
	margin: 7px 0px;
}

h1, h2, h3, .nav a {
	font-family: $headingfont;
}

p, li {
	color: $primarycolor;
	font-size: 1.3em;
	line-height: 1.4em;
}
.small {
	font-size: .9em
}
.transition, .secondary-section a, #main-content a, .class-info  a, footer a {
	-webkit-transition: all 0.5s linear;
	-moz-transition: all 0.5s linear;
	-ms-transition: all 0.5s linear;
	-o-transition: all 0.5s linear;
	transition: all 0.5s linear;
}
.transition-background-only li {
	-webkit-transition: background-color 0.5s linear;
	-moz-transition: background-color 0.5s linear;
	-ms-transition: background-color 0.5s linear;
	-o-transition: background-color 0.5s linear;
	transition: background-color 0.5s linear;
}
/*********************
Header
*********************/

header {
	width: 100%; /* 960px/960px */
	-webkit-text-shadow: 1px 1px $accentcolor, 2px 2px $accentcolor, 3px 3px $accentcolor, 4px 4px $accentcolor, 5px 5px $accentcolor;
	-moz-text-shadow: 1px 1px $accentcolor, 2px 2px $accentcolor, 3px 3px $accentcolor, 4px 4px $accentcolor, 5px 5px $accentcolor;
	-ms-text-shadow: 1px 1px $accentcolor, 2px 2px $accentcolor, 3px 3px $accentcolor, 4px 4px $accentcolor, 5px 5px $accentcolor;
	-o-text-shadow: 1px 1px $accentcolor, 2px 2px $accentcolor, 3px 3px $accentcolor, 4px 4px $accentcolor, 5px 5px $accentcolor;
	text-shadow: 1px 1px $accentcolor, 2px 2px $accentcolor, 3px 3px $accentcolor, 4px 4px $accentcolor, 5px 5px $accentcolor;
}
header h1 {
	color: $primarycolor;
	text-align: center;
	margin: 100px 0;
	font-size: 5em;
	line-height: 1.1em;
}
/*********************
Navigation
*********************/
nav {
	height: 80px;
	margin: 0;
	background-color: $accentcolor;
}
.nav { 
	margin: 25px 0 0 0; 
	width: 49.937578027466%; /* 400/801p */
	float: left;
	text-align: center;
}
.nav a, .nav a:visited {
	font-size: 2em;
	color: $primarycolor;
	text-decoration: none;
}	

.nav a:hover {
	text-decoration: none;
	-webkit-text-shadow: 1px 1px $secondarycolor, 2px 2px $secondarycolor, 3px 3px $secondarycolor, 4px 4px $secondarycolor, 5px 5px $secondarycolor;
	-moz-text-shadow: 1px 1px $secondarycolor, 2px 2px $secondarycolor, 3px 3px $secondarycolor, 4px 4px $secondarycolor, 5px 5px $secondarycolor;
	-ms-text-shadow: 1px 1px $secondarycolor, 2px 2px $secondarycolor, 3px 3px $secondarycolor, 4px 4px $secondarycolor, 5px 5px $secondarycolor;
	-o-text-shadow: 1px 1px $secondarycolor, 2px 2px $secondarycolor, 3px 3px $secondarycolor, 4px 4px $secondarycolor, 5px 5px $secondarycolor;
	text-shadow: 1px 1px $secondarycolor, 2px 2px $secondarycolor, 3px 3px $secondarycolor, 4px 4px $secondarycolor, 5px 5px $secondarycolor;

} 
/*********************
Main Content
*********************/

#main-content {
	width: 66.666666666666666666666666666667%; /* 640px/960px */
	padding: 0px;
	margin: 0px 0px 20px 0px;
	float: right;
}
#main-content a {
	color: $linkcolor;
	text-decoration: none;
	border-bottom: 2px dotted $accentcolor;
}
#main-content  a:visited {
	color: $linkcolor;
}
#main-content  a:hover {
	color: $linkcolor;
	text-decoration: none;
	border-bottom: 2px dotted $accentcolor;
}
#introduction {
	padding: 10px 3.125%; /* 20px/640px */
	margin: 0px;
}
#introduction p, #introduction h1 {
	color: $primarycolor;
}
#introduction a:hover {
	border-bottom: 2px dotted #ccc;
}
.emphasis {
	font-weight: bold;
}
.class-info {
	margin: 30px auto;
	padding: 10px 3.125%; /* 20px/640px */	
}
.clearfloat { 
	clear:both; 
}
.class-info h2 {
	border-bottom: 6px solid $primarycolor;
	margin-bottom: 15px;
	padding-bottom: 15px;
}
.class-info  p{
	color: $primarycolor;
	margin-bottom: 10px;
}
#main-content .class-info  a {
	color: $primarycolor;
	text-decoration: none;
	border-bottom: 2px dotted #fff;
}

#main-content .class-info  a:hover {
	text-decoration: none;
	border-bottom: 2px dotted $accentcolor;

}
#main-content .class-info  a:visited {
	color: $primarycolor;
}
#main-content .class-info  a.button {
	background-color: none;
	font-size: 1.3em; /* 14px/16px */
	font-family: $headingfont;
	margin: 10px 5px; 
	padding: 5px .94339622641509433962264150943396%; /* 5px/530px */
	border: none;
	-webkit-border-radius: 5px;
	-moz-border-radius: 5px;
	-o-border-radius: 5px;
	border-radius: 5px;
	line-height: 20px;
	float: right;
	border: 1px solid $primarycolor;
}

#main-content .class-info .button:hover {
	background-color: $accentcolor;
}

/*********************
Secondary Content
*********************/
#secondary-content {
	width: 30%; /* 230px/960px */
	border: none;
	padding: 0px;
	margin: 0px 0px 20px 0px;
	float: left;
}
h3.separation {
	margin: 10px 0px 10px -8.6956521739130434782608695652174%; /* -20px/230px */
	padding: 0 4.3478260869565217391304347826087%; /* 10px/230px */
}
.secondary-section { /* Total Width 230px */

	margin: 15px 0; 
	padding: 10px 0; /* 20px/190px */
}

.secondary-section a {
	color: $linkcolor;
	text-decoration: none;
}
.secondary-section a:visited {
	color: $linkcolor;
}
.secondary-section a:hover {
	color: $linkcolor;
	background-color: $accentcolor;
	text-decoration: none;
	border-bottom: 1px dotted $linkcolor;
}
.secondary-section p {
	color: $primarycolor;
}
#secondary-content li {
	list-style: disc;
	margin-left: 10px;
}
/*********************
footer
*********************/
footer {
	clear: both;
	height: 100px;
	background-color: $accentcolor;
	padding: 50px 0px 50px 0px;
	text-align: center;
}
footer  a {
	color: $linkcolor;
	text-decoration: none;
	padding: 1px;
	border-bottom: 2px dotted #fff;
}

footer a:hover {
	border-bottom: 2px dotted #111;
}
footer a:visited {
	color: $linkcolor;
}
footer b.highlight {
	font-weight: bold;
}

footer p:first-child {
	font-family: $headingfont;
	font-size: 1.4em;
	margin-bottom: 5px;
}


/*********************
Form and input styling
*********************/
#contact {
	padding: 10px 3.125%; /* 20px/640px */
	margin: 0px;
}
#contact p {
	padding: 20px 0;
}
#contact label {
	display: block;
	margin-top: 10px;
	font-size: 1.3em;
	font-family: $headingfont;
}
#contact input {
	display: block;
	padding-top: 8px;
	padding-bottom: 8px;
	font-size: 1.35em;
	color: $primarycolor;
}
#contact textarea {
	width: 95%;
	font-size: 1.3em;
	color: $primarycolor;
}
#contact a.button {
	color: #fff;
	background-color: $linkcolor;
	font-size: 1.5em; /* 14px/16px */
	font-family: $headingfont;
	margin: 10px 5px; 
	padding: 15px 5px; /* 5px/530px */
	-webkit-border-radius: 15px;
	-moz-border-radius: 15px;
	-o-border-radius: 15px;
	border-radius: 15px;
	line-height: 20px;
	border: none;
}
#contact .button:hover {
	background-color: $accentcolor;
}

/*****************************
----START OF MEDIA QUERIES----
*****************************/

/********************************************************
Media Query for Mobile Devices 850px or less (iPad, smart phones)
********************************************************/
@media all and (max-width: 950px) {
	#wrapper {
		width: 95%;
	}
 	#main-content {
		float: none;
		width: 100%;
		margin: 0 auto;
		border-bottom: 1px dotted $primarycolor;
	}
	#secondary-content {
		float: none;
		width: 95%;
		margin: 0 auto;
	}
	h3.separation {
		margin-left: 0;
		padding: 0 0 10px 0;
		border-bottom: 4px solid $primarycolor;
	}
	h3.separation:first-child {
		margin-top: 50px;
	}
	#secondary-content li {
		list-style: disc;
		margin-left: 40px;
	}
}



